use_frameworks!

def default_pods
    pod 'ManaKit', :path => '../'
    
    pod 'Kanna'
    pod 'PromiseKit'
    pod 'SDWebImage'
    pod 'Sync'
end

target 'ManaKit_Example' do
    default_pods
    platform :ios, "13.0"

    pod 'FontAwesome.swift'
    pod 'KeychainAccess'
    pod 'MBProgressHUD'
    pod 'PostgresClientKit', '~> 1.0'
end

target 'ManaKit_ExampleTests' do
    default_pods
    platform :ios, "13.0"

    pod 'FontAwesome.swift'
    pod 'KeychainAccess'
    pod 'MBProgressHUD'
    pod 'PostgresClientKit', '~> 1.0'
end

target 'ManaKit_ExampleUITests' do
    default_pods
    platform :ios, "13.0"

    pod 'FontAwesome.swift'
    pod 'KeychainAccess'
    pod 'MBProgressHUD'
    pod 'PostgresClientKit', '~> 1.0'
end
